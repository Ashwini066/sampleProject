<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test thread-count="3" name="Group Test">
		<parameter name="browser" value="Chrome"></parameter>

		<groups>
			<run>
				<include name="launch"></include>
				<include name="Login"></include>
				<include name="CheckDisplay"></include>
				<include name="close"></include>
			</run>
		</groups>
		<classes>
			<class name="testClasses.HomePageTestClass" />
			<class name="testClasses.LoginPageTestClass" />
			<class name="testClasses.RegisterAPatientTestClass" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
